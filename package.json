{
  "name": "lancelot",
  "version": "1.0.0",
  "description": "The custom discord bot for the Knight Hacks discord",
  "main": "src/index.ts",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development nodemon",
    "start:prod": "export NODE_ENV=production && tsc -p tsconfig.production.json && node --enable-source-maps ./dist/index.js",
    "test": "jest ./src/__tests__",
    "lint": "eslint --fix ./src",
    "format": "prettier --write ./src",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suneettipirneni/node-typescript-template.git"
  },
  "author": "Suneet Tipirneni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KnightHacks/Lancelot.git/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "homepage": "https://github.com/KnightHacks/Lancelot.git#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.2",
    "@babel/preset-typescript": "^7.16.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@sentry/cli": "^1.71.0",
    "@types/humps": "^2.0.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.0",
    "@types/node-fetch": "^2.6.2",
    "@types/node-schedule": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-jest": "^27.3.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@knighthacks/hackathon": "^0.1.0-dev-84bca6b",
    "@knighthacks/scythe": "^1.0.0",
    "@sentry/integrations": "^6.15.0",
    "@sentry/node": "^6.15.0",
    "@sentry/tracing": "^6.15.0",
    "@sentry/webpack-plugin": "^1.18.3",
    "axios": "^0.24.0",
    "bufferutil": "^4.0.5",
    "discord.js": "^14.7.1",
    "discord.js-embed-pagination": "^0.7.0",
    "dotenv": "^10.0.0",
    "humps": "^2.0.1",
    "node-schedule": "^2.1.0",
    "utf-8-validate": "^5.0.7",
    "zlib-sync": "^0.1.7"
  }
}
