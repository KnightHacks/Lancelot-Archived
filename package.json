{
  "name": "lancelot",
  "version": "1.0.0",
  "description": "The custom discord bot for the Knight Hacks discord",
  "main": "src/index.ts",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "yarn nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "start:prod": "export NODE_ENV=production && tsc -p tsconfig.production.json && node --enable-source-maps ./dist/index.js",
    "test": "vitest ./src/__tests__",
    "lint": "eslint --fix ./src",
    "format": "prettier --write ./src",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suneettipirneni/node-typescript-template.git"
  },
  "author": "Suneet Tipirneni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KnightHacks/Lancelot.git/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "homepage": "https://github.com/KnightHacks/Lancelot.git#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@sentry/cli": "^1.71.0",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.25",
    "@types/humps": "^2.0.1",
    "@types/node": "^16.11.0",
    "@types/node-schedule": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@knighthacks/hackathon": "^0.1.0-dev-84bca6b",
    "@knighthacks/scythe": "^1.0.2",
    "@sentry/integrations": "^7.29.0",
    "@sentry/node": "^7.29.0",
    "@sentry/tracing": "^7.29.0",
    "@sentry/webpack-plugin": "^1.20.0",
    "bufferutil": "^4.0.7",
    "discord.js": "^14.7.1",
    "discord.js-embed-pagination": "^0.7.0",
    "dotenv": "^10.0.0",
    "humps": "^2.0.1",
    "node-schedule": "^2.1.0",
    "undici": "^5.14.0",
    "utf-8-validate": "^6.0.0",
    "vitest": "^0.27.0"
  }
}
