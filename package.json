{
  "name": "lancelot",
  "version": "1.0.0",
  "description": "The custom discord bot for the Knight Hacks discord",
  "main": "src/index.ts",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development nodemon --exec ts-node --files ./src/index.ts",
    "start:prod": "export NODE_ENV=production && tsc && node ./dist/index.js",
    "test": "jest ./src/__tests__",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suneettipirneni/node-typescript-template.git"
  },
  "author": "Suneet Tipirneni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suneettipirneni/node-typescript-template/issues"
  },
  "homepage": "https://github.com/suneettipirneni/node-typescript-template#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@knighthacks/dispatch": "^0.1.0-dev-42f61ad",
    "axios": "^0.21.1",
    "discord.js": "^13.0.0",
    "dotenv": "^10.0.0"
  }
}
