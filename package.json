{
  "name": "lancelot",
  "version": "1.0.0",
  "description": "The custom discord bot for the Knight Hacks discord",
  "main": "src/index.ts",
  "engines": {
    "node": ">=16.6.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development nodemon --exec ts-node --files ./src/index.ts",
    "start:prod": "export NODE_ENV=production && tsc -p tsconfig.production.json && node ./dist/index.js",
    "test": "jest ./src/__tests__",
    "lint": "eslint --fix ./src",
    "format": "prettier --write ./src",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suneettipirneni/node-typescript-template.git"
  },
  "author": "Suneet Tipirneni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KnightHacks/Lancelot.git/issues"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "homepage": "https://github.com/KnightHacks/Lancelot.git#readme",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@sentry/cli": "^1.69.1",
    "@types/humps": "^2.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-jest": "^27.2.5",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "ts-jest": "^27.0.6",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@knighthacks/hackathon": "^0.1.0-dev-84bca6b",
    "@knighthacks/scythe": "^0.1.0-dev-27c6e8f",
    "@sentry/integrations": "^6.13.3",
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "@sentry/webpack-plugin": "^1.18.1",
    "axios": "^0.22.0",
    "bufferutil": "^4.0.3",
    "discord.js": "^13.2.0",
    "discord.js-embed-pagination": "^0.6.0",
    "dotenv": "^10.0.0",
    "erlpack": "github:discord/erlpack",
    "humps": "^2.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "utf-8-validate": "^5.0.7",
    "zlib-sync": "^0.1.7"
  }
}
